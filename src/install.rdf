<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>${extension.guid}</em:id>
    <em:version>${extension.fullversion}</em:version>
    
    <!-- Target Application this extension can install into, 
        with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.0+</em:minVersion>
        <em:maxVersion>3.0b4pre</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Front End MetaData -->
    <em:name>JavaScript Options</em:name>
    <em:description>Provides advanced JavaScript options for Firefox.</em:description>
    <em:creator>Mossop</em:creator>
    <em:contributor>Andrea &amp; Luana (Italian translation)</em:contributor>
    <em:contributor>Andreas Grundler (German translation)</em:contributor>
    <em:contributor>Xavier Robin (French translation)</em:contributor>
    <em:contributor>Daniel Crompton (Dutch translation)</em:contributor>
    <em:homepageURL>http://www.oxymoronical.com/web/firefox/${extension.name}</em:homepageURL>
    <em:optionsURL>chrome://jsoptions/content/jsoptions.xul</em:optionsURL>
    <em:iconURL>chrome://jsoptions/skin/icon.png</em:iconURL>
    
  </Description>      
</RDF>
