<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{cdbbb3f6-a50e-4b20-a154-5fcbb3bbf43d}</em:id>
    <em:version>1.0</em:version>
    
    <!-- Target Application this extension can install into, 
        with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>1.1+</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Front End MetaData -->
    <em:name>JavaScript Options</em:name>
    <em:description>Provides advanced JavaScript options in the new preferences window.</em:description>
    <em:creator>Dave Townsend</em:creator>
    <em:homepageURL>http://www.blueprintit.co.uk/~dave/web/firefox/jsoptions/index.html</em:homepageURL>
    <em:updateURL>http://www.blueprintit.co.uk/~dave/web/firefox/jsoptions/jsoptions.rdf</em:updateURL>
    <em:iconURL>chrome://jsoptions/skin/icon.png</em:iconURL>
    <em:optionsURL>chrome://jsoptions/content/jsoptions.xul</em:optionsURL>
    
    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:jsoptions.jar">
        <em:package>content/</em:package>
        <em:skin>skin/</em:skin>
        <em:locale>locale/en-US/</em:locale>
      </Description>
    </em:file>

  </Description>      
</RDF>
